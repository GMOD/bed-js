{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "globals": {
    "window": true,
    "__webpack_require__": true
  },
  "env": {
    "jest": true
  },
  "rules": {
    "prefer-destructuring": 0,
    "import/prefer-default-export": 0,
    "curly": "error",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "all"
      }
    ]
  }
}
