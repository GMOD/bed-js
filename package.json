{
  "name": "@gmod/bed",
  "version": "0.0.1",
  "description": "A BED file format parser with autoSql support",
  "main": "dist/index.js",
  "repository": "github:GMOD/bed-js",
  "author": {
    "name": "Colin Diesh",
    "email": "colin.diesh@gmail.com",
    "url": "https://github.com/cmdcolin"
  },
  "license": "MIT",
  "homepage": "https://github.com/GMOD/bed-js",
  "bugs": {
    "url": "https://github.com/GMOD/bed-js/issues"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src test",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "babel -d src",
    "patch": "npm version patch && npm publish --access public",
    "minor": "npm version minor && npm publish --access public",
    "major": "npm version major && npm publish --access public",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push origin master --follow-tags"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "documentation": "^9.1.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^23.5.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.14.2",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "pegjs": "^0.10.0",
    "snake-case": "^2.1.0"
  },
  "keywords": [
    "bed",
    "autosql",
    "genomics",
    "bionode",
    "biojs"
  ],
  "publishConfig": {
    "access": "public"
  }
}
