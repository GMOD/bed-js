{
  "presets": [["@babel/preset-env", {
      "targets": {
        "node": 6,
        "browsers": [
          "> 0.5%",
          "last 2 versions"
        ]
      }
    }
  ]],
  "plugins": [
    ["@babel/transform-runtime", {
      "regenerator": true
    }],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-throw-expressions",
  ]
}
